{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Access-Control-Allow-Origin", "value": "false" },
                { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS, POST, PUT, DELETE" },
                { "key": "Access-Control-Allow-Headers", "value": "*" },
                { "key": "Access-Control-Allow-Credentials", "value": "true" }
            ]
        }
    ]
}
